<Window x:Class="Launcher.LoginPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Launcher"
        mc:Ignorable="d"
         Title="Login" Width="300" Height="300">
    <Grid Background="#FF46AC28">
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Top">
            <Label/>
            <Label/>
            <Label/> 
            <Label/>
            <Label/>
            <Label Content="Email" Name="Email"/>
            <TextBox Width="200" Name="EmailBox"/>
            <Label Content="Password"/>
            <PasswordBox Width="200"  Name="Password"/>
            <Label/>
            <Label/>
            <Label/> 
            <Button x:Name="LoginButton" Content="Login" Click="LoginButton_Click"/>
        </StackPanel>
    </Grid>
</Window>
